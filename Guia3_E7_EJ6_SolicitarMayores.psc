// Programa: SolicitarMayores
Proceso SolicitarMayores
	Definir primer_numero, numero Como Real;
	Escribir "Ingrese el primer número: ";
	Leer primer_numero;
	Escribir "Ingrese un número: ";
	Leer numero;
	Mientras numero > primer_numero Hacer
		Escribir "Ingrese un número: ";
		Leer numero;
	FinMientras
FinProceso

